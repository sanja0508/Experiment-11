#include <iostream>
using namespace std;

class Rectangle
{
    public:
    
    int length;
    int width;
    
    int area()
    {
        return length*width;
    }
};

int main() 
{
    Rectangle r1;
    
    r1.length=25;
    r1.width=41;
    
    cout<<r1.length<<endl;
    cout<<r1.width<<endl;
    cout<<r1.area()<<endl;
    
    return 0;
}
