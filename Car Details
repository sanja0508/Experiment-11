#include <iostream>
using namespace std;
class Car
{
    public:
    
    string name;
    string colour;
    float year;
};

int main() 
{
    Car s1;
    
    s1.name="Hero";
    s1.colour="Red";
    s1.year=2016;
    
    cout<<s1.name<<endl;
    cout<<s1.colour<<endl;
    cout<<s1.year<<endl;
    
    
    Car s2;
    
    s2.name="Honda";
    s2.colour="Black";
    s2.year=2020;
    
    cout<<s2.name<<endl;
    cout<<s2.colour<<endl;
    cout<<s2.year<<endl;
    
    return 0;
}
